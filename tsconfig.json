{
    "compilerOptions": {
        "target": "ES5",
        "lib": [
            "es2015",
            "es2017"
        ],
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "typeRoots": [
            "node_modules/@types",
            "src/@types"
        ],
        "types": [
            "./google-apps-script",
            "./global"
        ],
    },
    "include": [
        "src/**/*.ts"
    ]
}